    </bean>
    
    <bean id="address" class="study.pattern.vaccination.domain.Address">
      <property name="pinCode" value="544191"/>
      <property name="line1" value="sengkang"/>
      <property name="line2" value="singapore"/>
    </bean>
  
     
</beans> 